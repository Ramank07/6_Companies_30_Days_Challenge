class Solution {
public:
    int maxRotateFunction(vector<int>& nums) {
        int sum=0;
        int ans=0;
        int arrsum=0;

        int n=nums.size();
        for(int i=0; i<n; i++){
            sum+=nums[i]*i;
            arrsum+=nums[i];
        }
        ans=sum;
        int k=n-1;
        while(k){
            sum+=(arrsum-(n)*nums[k]);
            cout<<sum<<" ";
            ans=max(ans,sum);
            k-=1;
        }
        return ans;
    }
};